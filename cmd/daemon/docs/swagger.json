        "/config": {
            "get": {
                "description": "Retrieves configuration for a given project.",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "config"
                ],
                "summary": "Get configuration",
                "parameters": [
                    {
                        "type": "boolean",
                        "description": "Get content or values, default is false",
                        "name": "content",
                        "in": "query"
                    },
                    {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "collectionFormat": "csv",
                        "description": "Values to retrieve, default is all",
                        "name": "values",
